{
    "id": "core.app_launcher",
    "name": "App Launcher",
    "description": "Dynamic app launcher using semantic category matching.",
    "version": "1.1.0",
    "author": "AVVA Core",
    "intents": {
        "regex:(?:launch|open|start|run)\\s+(.*)": "launch_application(\"$1\")",
        "regex:^([a-z0-9_-]+)$": "launch_application(\"$1\")"
    },
    "entry_point": "main.py",
    "permissions": [
        "apps.launch"
    ]
}